/* components/empty-page/empty-page.wxss */
.empty {
  width: 100%;
  height: 100vh;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
}

.empty image {
  width: 50%;
  height: 250rpx;
}

.title {
  font-size: 32rpx;
  margin-top: 60rpx;
  margin-bottom: 20rpx;
}

.content {
  text-align: center;
  width: 60%;
  font-size: 26rpx;
  color: #aaa;
}