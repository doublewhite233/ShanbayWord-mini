/* pages/userasset/userasset.wxss */
#background {
  width: 100%;
  height: 200rpx;
  position: absolute;
  z-index: -100;
}

.title{
  font-size: 28rpx;
  color: #888;
  padding-top: 80rpx;
}

.title-main {
  font-size: 46rpx;
  padding-left: 50rpx;
  padding-right: 10rpx;
  color: #000;
}

.content {
  text-align: center;
  font-size: 80rpx;
  margin-top: 100rpx;
}

.content image {
  height: 80rpx;
  width: 80rpx;
}

#buyBtn {
  font-weight: normal;
  font-size: 34rpx;
  color: #fff;
  background-color: #44cc99;
  border-radius: 50rpx;
  width: 60%;
  margin-top: 80rpx;
  margin-bottom: 100rpx;
}

.foot {
  font-size: 26rpx;
}

.foot view {
  margin: 20rpx 50rpx;
}

.foot-title {
  text-align: center;
  color: #888;
  padding-bottom: 10rpx;
}

.foot-con::before {
  content: ' ';
  border-radius: 50%;
	width: 10rpx;
	height: 10rpx; 
  background-color: #44cc99;
  color: #44cc99;
  position: absolute;
  margin-top: 10rpx;
  margin-left: -20rpx;
  }